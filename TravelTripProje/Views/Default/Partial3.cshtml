@using TravelTripProje.Models.Sınıflar
@model List<Blog>

<div class="padding">
    <h4>Haftanın en iyi 10 bloğu</h4>
    <ul class="list list-pad">
        @{ int i = 0;}
        @foreach (var P in Model)
        {
            <li class="list-bg"><span>@{i++;} @i.</span><a href="#">@P.Baslık </a></li>
        }
    </ul>
    @*<a href="#" class="link1">See all hotels</a>*@
</div>